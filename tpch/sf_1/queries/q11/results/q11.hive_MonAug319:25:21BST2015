set hive.execution.engine=mr;

DROP TABLE q11_important_stock;
DROP TABLE q11_part_tmp;
DROP TABLE q11_sum_tmp;

-- create the target table
create table q11_important_stock(ps_partkey INT, value DOUBLE) STORED AS ORC;
create table q11_part_tmp(ps_partkey int, part_value double) STORED AS ORC;
create table q11_sum_tmp(total_value double) STORED AS ORC;

-- the query
insert into table q11_part_tmp
select 
  ps_partkey, 
  sum(ps_supplycost * ps_availqty) as part_value 
from
  nation n join supplier s on s.s_nationkey = n.n_nationkey and n.n_name = 'RUSSIA'
  join partsupp ps on ps.ps_suppkey = s.s_suppkey
group by 
	ps_partkey;

insert into table q11_sum_tmp
select 
  sum(part_value) as total_value
from 
  q11_part_tmp;

insert into table q11_important_stock
select 
  ps_partkey, part_value as value
from
  (
    select ps_partkey, part_value, total_value
    from q11_part_tmp join q11_sum_tmp
  ) a
where part_value > total_value * 0.0001
order by value desc;

